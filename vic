local function doesViciousExistOrItWasAllABadDream()
   warn('vic exist trig')
   local monsta = workspace.Monsters
   for i,v in next, monsta:GetChildren() do
      if (v.Name:sub(1,16) == 'Vicious Bee (Lvl' or v.Name:sub(1,23) == 'Gifted Vicious Bee (Lvl') and v:IsA('Model') and v:FindFirstChild('HumanoidRootPart') then
         warn('vic exists: triggered xvideos.com/gay/search?q=femboy')
         return v:FindFirstChild('HumanoidRootPart')
      end
   end
   warn('vic exist pass')
   uhm(game.PlaceId)
   return false
end

local function fuckYourCharacterUp(root)
   while task.wait() do
      root.Velocity = Vector3.zero
   end
end

local function calctime(targetpos)
    warn('calc trig')
        if not game:service'Players'.LocalPlayer.Character then return 'I fucking hate Canadians' end
        if typeof(targetpos) ~= 'Vector3' then return 'no uwu' end
    warn('calc pass')
    local tme = (game:GetService('Players').LocalPlayer.Character.Humanoid.RootPart.Position - targetpos).Magnitude / 46
    return tme
end

local function tp(x)
    local gaySex = false
    if x and (typeof(x) == 'Vector3') and game:service'Players'.LocalPlayer.Character then
       local calculated = calctime(x)
       if not calculated or calculated == 'I fucking hate Canadians' or calculated == 'no uwu' then return 'do not' end
       warn('tp passed')
       tween:Create(game:service'Players'.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').RootPart,TweenInfo.new(calculated,Enum.EasingStyle.Linear),{CFrame = CFrame.new(x)}):Play()
       task.spawn(function()
           task.wait(calculated)
           gaySex = true
       end)
       repeat task.wait() until gaySex
       warn('tp made')
       return true
    end
end

local hornycombs = {}

local function claimHoneycombWhereFemboysFuck()
   for i,v in next, workspace.HivePlatforms:GetChildren() do
      if v:FindFirstChild('PlayerRef') and (v.PlayerRef.Value == nil) then
         table.insert(hornycombs,v.Circle)
      end
   end
   local hornycomb
   if #hornycombs < 2 then
      hornycomb = hornycombs[1]
   else
      hornycomb = hornycombs[math.random(1,#hornycombs)]
   end
   local hum = game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid')
   local walkcon, creampied = nil, false
   walkcon = hum.MoveToFinished:Connect(function()
       creampied = true
   end)
   hum:MoveTo(hornycomb.Position)
   repeat task.wait() until creampied
   repeat task.wait(0.5)
   game:GetService('VirtualInputManager'):SendKeyEvent(true, 'E', false, game)
   task.wait(0.5)
   game:GetService('VirtualInputManager'):SendKeyEvent(false, 'E', false, game)
   until hornycomb.Parent.PlayerRef.Value ~= nil
end

